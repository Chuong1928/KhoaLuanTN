<div class="category-wrapper mt-4">
    <div class="row" id="items">
        <% @categorys.each do |category|  %>
            <%= render partial: 'categories/item', locals: {category: category}%>
        <% end %>
    </div> 
    <div class="text-right">
        <a href="/categories" class="btn btn-link px-0">Xem tất cả <i class='bx bx-right-arrow-alt'></i></a>
    </div> 
</div>